@model Programer.Core.ViewModels.ArticleGroups.ArticleGroupCreateOrEditVm
@{
    ViewData["Title"] = $"Group {@Model.Title}";
}
<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>@ViewData["Title"]</title>
</head>
<body>

    <div>
        <h4>@ViewData["Title"]</h4>
        <hr />
        <dl class="row">
            <dt class=" col-sm-2">
                @Html.DisplayNameFor(model => model.Title)
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.Title)
            </dd>
            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.CreateDate)
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.CreateDate)
            </dd>

        </dl>
    </div>
    <div>
        <a asp-action="Edit" asp-route-id="@Model.Id" type="button" class="btn btn-success">Edit</a> |
        <a asp-action="Index" type="button" class="btn btn-success">Back</a>
    </div>
</body>
</html>

